  "dependencies": {
    "d3": "^7.9.0",
    "d3-selection": "3",
    "jolokia.js": "workspace:^"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "jest": "^29.7.0",
    "rollup": "^4.20.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
